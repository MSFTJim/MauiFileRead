<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiFileRead.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiFileRead"
    Shell.FlyoutBehavior="Flyout"
    Title="MauiFileRead">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
     Title="About"
     ContentTemplate="{DataTemplate local:About}"
     Route="About" />

    <ShellContent
 Title="About Button"
 ContentTemplate="{DataTemplate local:AboutButton}"
 Route="AboutButton" />

</Shell>
